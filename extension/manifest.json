{
  "manifest_version": 2,
  "name": "Parameter Proxy Extension",
  "description": "",
  "version": "0.0.0.1",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Content-Security-Policy headers are enabled",
    "default_icon": {
      "128":"icon.png"
    }
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "*://pbs.twimg.com/media/",
    "*://video.twimg.com/tweet_video/",
    "*://twitter.com/",
    "*://mobile.twitter.com/",
    "*://tweetdeck.twitter.com/",
    "*://api.twitter.com/",
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts": [
      "script.js"
    ],
    "persistent": true
  }
}